{
  "name": "autoback-cl",
  "version": "2.0.8",
  "description": "Simple autoback mechanism for web sites",
  "main": "dist/autoback-cl.js",
  "module": "src/autoback.js",
  "scripts": {
    "build:dev": "npx webpack --config webpack.dev.js",
    "build:prod": "npx webpack --config webpack.prod.js",
    "build:all": "npm run build:dev && npm run build:prod",
    "serve": "webpack serve --config webpack.dev.js --open"
  },
  "keywords": [
    "User interface",
    "Toggle"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charles-owen/autoback-cl.git"
  },
