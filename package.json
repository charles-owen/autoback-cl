{
  "name": "autoback-cl",
  "version": "2.2.2",
  "description": "Simple autoback mechanism for web sites",
  "main": "dist/autoback-cl.js",
  "module": "src/autoback.js",
  "scripts": {
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:all": "$npm_execpath run build:dev && $npm_execpath run build:prod",
    "serve": "webpack serve --config webpack.dev.js --open"
  },
  "keywords": [
    "User interface",
    "Toggle"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charles-owen/autoback-cl.git"
  },
  "author": "Charles B. Owen",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/runtime": "7.16.5",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "css-what": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "url-loader": "^4.1.1",
    "url-parse": "^1.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "4.7.1",
    "webpack-merge": "^5.8.0"
  }
}
